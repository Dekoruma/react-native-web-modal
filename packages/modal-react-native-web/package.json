    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "prop-types": "^15.6.2",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
